    <copyright>Copyright (C) 2013 Solidres. All Rights Reserved.</copyright>
    <reference type="content">
        <table name="sr_reservation_assets">
            <field type="referenceid" name="id" translate="0">ID</field>
            <field type="titletext" name="name"  length="50" maxlength="255" translate="1">Name</field>
            <field type="htmltext" name="description" translate="1">Description</field>
            <field type="text" name="address_1" length="50" maxlength="255" translate="1">Address 1</field>
            <field type="text" name="address_2" length="50" maxlength="255" translate="1">Address 2</field>
            <field type="text" name="city"  length="50" maxlength="255" translate="1">City</field>
            <field type="text" name="alias"  length="50" maxlength="255" translate="1">Alias</field>
            <field type="textarea" name="metatitle" translate="1">Page title</field>
            <field type="textarea" name="metadesc" translate="1">Meta Description</field>
            <field type="textarea" name="metakey" translate="1">Meta Keywords</field>
            <field type="text" name="xreference" translate="1">External Reference</field>
        </table>
    </reference>
    <translationfilters>
		<keyword>name</keyword>
    </translationfilters>
</falang>
